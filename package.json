{
  "name": "soc-data",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "postinstall": "tsc",
    "watch-node": "nodemon dist/index.js",
    "watch-ts": "tsc -w",
    "deploy": "git add . && git commit -m Heroku && git push heroku master",
    "dbCreateBootcampsTable": "node -r dotenv/config dist/db/scripts/bootcamps/createTable.js",
    "dbDeleteBootcampsTable": "node -r dotenv/config dist/db/scripts/bootcamps/dropTable.js",
    "dbPopulateBootcampsTable": "node -r dotenv/config dist/db/scripts/bootcamps/populateTable.js",
    "dbResetBootcampsTable": "npm run dbDeleteBootcampsTable && npm run dbCreateBootcampsTable && npm run dbPopulateBootcampsTable",
    "dbCreateAssignmentsTable": "node -r dotenv/config dist/db/scripts/assignments/createTable.js",
    "dbDeleteAssignmentsTable": "node -r dotenv/config dist/db/scripts/assignments/dropTable.js",
    "dbPopulateAssignmentsTable": "node -r dotenv/config dist/db/scripts/assignments/populateTable.js",
    "dbResetAssignmentsTable": "npm run dbDeleteAssignmentsTable && npm run dbCreateAssignmentsTable && npm run dbPopulateAssignmentsTable"
  },
  "dependencies": {
    "@types/pg": "^8.6.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "pg": "^8.7.1",
    "typescript": "^4.4.4",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.3"
  },
  "engines": {
    "node": "14.17.4"
  }
}
