{
  "name": "soc-data",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node -r dotenv/config dist/index.js",
    "compile": "tsc && node -r dotenv/config dist/index.js",
    "test": "jest",
    "dev": "./node_modules/nodemon/bin/nodemon.js -e ts  --exec \"npm run compile\"",
    "postinstall": "tsc",
    "watch-node": "nodemon r dotenv/config dist/index.js",
    "watch-ts": "tsc -w",
    "deploy": "git add . && git commit -m Heroku && git push heroku master",
    "dbCreateBootcampsTable": "node -r dotenv/config dist/db/scripts/bootcamps/createTable.js",
    "dbDeleteBootcampsTable": "node -r dotenv/config dist/db/scripts/bootcamps/dropTable.js",
    "dbPopulateBootcampsTable": "node -r dotenv/config dist/db/scripts/bootcamps/populateTable.js",
    "dbResetBootcampsTable": "npm run dbDeleteBootcampsTable && npm run dbCreateBootcampsTable && npm run dbPopulateBootcampsTable",
    "dbCreateAssignmentsTable": "node -r dotenv/config dist/db/scripts/assignments/createTable.js",
    "dbDeleteAssignmentsTable": "node -r dotenv/config dist/db/scripts/assignments/dropTable.js",
    "dbPopulateAssignmentsTable": "node -r dotenv/config dist/db/scripts/assignments/populateTable.js",
    "dbResetAssignmentsTable": "npm run dbDeleteAssignmentsTable && npm run dbCreateAssignmentsTable && npm run dbPopulateAssignmentsTable",
    "dbCreateStudentsTable": " node -r dotenv/config dist/db/scripts/students/createTable.js",
    "dbDropStudentTable": "node -r dotenv/config dist/db/scripts/students/dropTable.js",
    "dbPopulateStudentTable": "node -r dotenv/config dist/db/scripts/students/populateTable.js",
    "dbResetStudentsTable": " npm run dbDropStudentTable && npm run dbCreateStudentsTable &&  npm run dbPopulateStudentTable ",
    "dbCreateUsersTable": " node -r dotenv/config dist/db/scripts/users/createTable.js",
    "dbDropUsersTable": "node -r dotenv/config dist/db/scripts/users/dropTable.js",
    "dbResetUsersTable": " npm run dbDropUsersTable && npm run dbCreateUsersTable",
    "createCommentsTable": "node -r dotenv/config dist/db/scripts/comments/createTable.js",
    "deleteCommentsTable": "node -r dotenv/config dist/db/scripts/comments/dropTable.js",
    "populateCommentsTable": "node -r dotenv/config dist/db/scripts/comments/populateTable.js"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/pg": "^8.6.1",
    "axios": "^0.24.0",
    "babel-jest": "^27.3.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "node-fetch": "^3.0.0",
    "pg": "^8.7.1",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.3",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": "14.17.4"
  }
}
